<Window x:Class="PowerCube.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="650" Width="525">
    <Grid>
        <TreeView x:Name="treeview1" HorizontalAlignment="Left" Height="592" Margin="11,10,0,0" VerticalAlignment="Top" Width="497" RenderTransformOrigin="0.5,0.5">
            <TreeView.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.286"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.745"/>
                </TransformGroup>
            </TreeView.RenderTransform>
        </TreeView>

    </Grid>
</Window>
